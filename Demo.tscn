[sub_resource type="GDScript" id=2]
script/source = "extends Button

func _pressed():
	if get_parent().pause_time < 49.37:
		print(\"Reloading disc 1 shader\")
		var loaded = ResourceLoader.load(\"Demo.shader\", \"Shader\", true)
		get_parent().material.shader = loaded
	else:
		print(\"Reloading disc 2 shader\")
		var loaded = ResourceLoader.load(\"DemoDisc2.shader\", \"Shader\", true)
		get_parent().get_parent().find_node(\"MarchTargetDisc2\").material.shader = loaded

func _input(event):
	if event is InputEventKey and event.pressed and event.get_scancode_with_modifiers() == (KEY_MASK_CTRL | KEY_R):
		_pressed()
"

